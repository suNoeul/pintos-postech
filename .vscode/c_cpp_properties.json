{
    "configurations": [
        {
            "name": "Pintos",
            "includePath": [
                "${workspaceFolder}/**"
            ],
            "defines": [
                "USERPROG",  // 전처리기 플래그 정의
                "FILESYS"
            ],
            "compilerPath": "/usr/bin/gcc",  // 사용 중인 컴파일러 경로 (리눅스 예시)
            "cStandard": "c11",  // C 표준 설정
            "cppStandard": "c++17"  // C++ 표준 설정 (필요 시)
        }
    ],
    "version": 4
}